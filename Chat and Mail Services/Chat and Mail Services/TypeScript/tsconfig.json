{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "removeComments": true,
    "noEmitOnError": true,
    "strict": false,
    "alwaysStrict": false,
    "noImplicitAny": false,
    "outDir": "../wwwroot/lib/"
  },
    "exclude": [
      "node_modules",
      "wwwroot"
    ]
  }